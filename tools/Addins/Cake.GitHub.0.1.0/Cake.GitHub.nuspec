<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Cake.GitHub</id>
    <version>0.1.0</version>
    <authors>Geert van Horrik, Cake Contributors</authors>
    <owners>Geert van Horrik, Cake Contributors</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/cake-contrib/Cake.GitHub</projectUrl>
    <iconUrl>https://cdn.jsdelivr.net/gh/cake-contrib/graphics/png/cake-contrib-medium.png</iconUrl>
    <description>GitHub Addin for Cake Build Automation System.</description>
    <copyright>Copyright 2017 - 2018 (c) Geert van Horrik and contributors</copyright>
    <tags>Cake Script Build GitHub</tags>
    <repository type="GitHub" url="https://github.com/cake-contrib/Cake.GitHub.git" />
    <dependencies>
      <group targetFramework=".NETFramework4.6">
        <dependency id="Microsoft.CSharp" version="4.4.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="11.0.2" exclude="Build,Analyzers" />
        <dependency id="Octokit" version="0.36.0" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.CSharp" version="4.4.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="11.0.2" exclude="Build,Analyzers" />
        <dependency id="Octokit" version="0.36.0" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>